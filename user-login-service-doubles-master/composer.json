{
    "name": "deg540/php-testing-boilerplate",
    "description": "Php testing boilerplate",
    "type": "project",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.10",
        "mockery/mockery": "^1.5",
        "phpro/grumphp": "^1.15",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmd/phpmd": "^2.13"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UserLoginService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserLoginService\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "scripts": {
        "test": [
          "phpunit --configuration phpunit.xml"
        ]
    }
}
